<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Small and simple disposable wrappers for ReaderWriterLockSlim</Description>
    <Copyright>Copyright (c) 2020 Roman Vovk</Copyright>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/wolf8196/ReaderWriterLockSlimDisposableWrapper</PackageProjectUrl>
    <RepositoryUrl>https://github.com/wolf8196/ReaderWriterLockSlimDisposableWrapper</RepositoryUrl>
    <PackageTags>readerwriterlockslim disposable</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
